{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach Chrome",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "url": "http://localhost:3000/",
            "webRoot": "${workspaceRoot}/src"
        },
        {
            "name": "Open Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceRoot}/src"
        }
    ]
}